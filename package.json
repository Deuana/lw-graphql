{
  "name": "lw-graphql",
  "version": "1.0.0",
  "description": "A web development homework | graphql side",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build",
    "start": "node dist/index.js",
    "build": "npm run clean && npm run build:server",
    "clean": "rm -rf dist && mkdir dist",
    "build:server": "./node_modules/.bin/babel src -d dist",
    "dev": "nodemon src/index.js --exec babel-node --presets es2015,stage-0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erikvinicius5/lw-graphql.git"
  },
  "keywords": [
    "web",
    "lab",
    "graphql"
  ],
  "author": "Anderson Sampaio, Deuana Teles, Erik Vin√≠cius, Rafael Dourado and Ted Ribeiro",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/erikvinicius5/lw-graphql/issues"
  },
  "homepage": "https://github.com/erikvinicius5/lw-graphql#readme",
  "devDependencies": {
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-transform-decorators": "^6.13.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "body-parser": "^1.15.2",
    "express": "^4.14.0"
  }
}
